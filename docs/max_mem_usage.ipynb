   "source": [
    "import pandas\n",
    "\n",
    "# find max memory usage\n",
    "df=pandas.read_csv('bench_results.csv')\n",
    "df['mem_usage']=df['mem_usage'].apply(lambda x: max(eval(x)))\n",
    "\n",
    "# convert to MB\n",
    "df['mem_usage']=df['mem_usage'].apply(lambda x: x//10**6)\n",
    "\n",
    "df.to_csv('bench_results_max.csv', index=False)"
   ]
